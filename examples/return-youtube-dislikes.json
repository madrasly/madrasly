    "/Votes": {
      "get": {
        "summary": "Get votes for a YouTube video",
        "description": "Retrieves vote statistics (likes, dislikes, rating, view count) for a given YouTube video ID. This is the primary endpoint for fetching vote data from the Return YouTube Dislike API. Returns comprehensive vote information including the video ID, creation date, like/dislike counts, rating, view count, and deletion status.",
        "tags": [
          "Votes"
        ],
        "parameters": [
          {
            "name": "videoId",
            "in": "query",
            "required": true,
            "description": "The YouTube video ID to fetch votes for (e.g., kxOuG8jMIgI). Must be a valid YouTube video ID format.",
            "schema": {
              "type": "string"
            },
            "examples": {
              "example": {
                "value": "kxOuG8jMIgI",
                "summary": "Example YouTube video ID"
              }
            }
          },
          {
            "name": "likeCount",
            "in": "query",
            "description": "Optional like count parameter",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully retrieved vote data. Returns an object containing the video ID, dateCreated (ISO 8601 format), likes count, dislikes count, rating (calculated value), viewCount, and deleted status (boolean).",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "description": "YouTube video ID"
                    },
                    "dateCreated": {
                      "type": "string",
                      "format": "date-time",
                      "description": "Date when the video data was created (ISO 8601 format)"
                    },
                    "likes": {
                      "type": "integer",
                      "description": "Number of likes for the video"
                    },
                    "dislikes": {
                      "type": "integer",
                      "description": "Number of dislikes for the video"
                    },
                    "rating": {
                      "type": "number",
                      "description": "Calculated rating value"
                    },
                    "viewCount": {
                      "type": "integer",
                      "description": "Total view count for the video"
                    },
                    "deleted": {
                      "type": "boolean",
                      "description": "Whether the video has been deleted"
                    }
                  }
                },
                "example": {
                  "id": "kxOuG8jMIgI",
                  "dateCreated": "2021-12-20T12:25:54.418014Z",
                  "likes": 27326,
                  "dislikes": 498153,
                  "rating": 1.212014408444885,
                  "viewCount": 3149885,
                  "deleted": false
                }
              }
            }
          },
          "400": {
            "description": "Bad Request - The provided YouTube video ID is incorrectly formatted"
          },
          "404": {
            "description": "Not Found - The provided YouTube video ID is invalid or does not exist"
          }
        }
      },
      "post": {
        "tags": [
          "Votes"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "string"
                }
