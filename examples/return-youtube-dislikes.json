    "/Interact/vote": {
      "post": {
        "summary": "Submit a vote for a YouTube video",
        "description": "Submits a like or dislike vote for a specified YouTube video. This endpoint is used to record user votes in the Return YouTube Dislike system. Requires a VoteModel containing the user ID, video ID, vote value (typically 1 for like, -1 for dislike), and optionally a puzzle solution for verification.",
        "tags": [
          "Interact"
        ],
        "requestBody": {
          "required": true,
          "description": "Vote data including user ID, video ID, vote value, and optional puzzle solution",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VoteModel"
              },
              "example": {
                "userId": "user123",
                "videoId": "kxOuG8jMIgI",
                "value": 1,
                "solution": null
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/VoteModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/VoteModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vote successfully submitted"
          },
          "400": {
            "description": "Bad Request - Invalid vote data or missing required fields"
          },
          "401": {
            "description": "Unauthorized - Authentication required or invalid credentials"
          }
        }
      }
    },
    "/Interact/confirmVote": {
      "post": {
        "summary": "Confirm a previously submitted vote",
        "description": "Confirms a vote that was previously submitted via the /Interact/vote endpoint. This endpoint is typically used as part of a two-step voting process to verify and finalize votes, often requiring puzzle solution verification to prevent automated voting abuse.",
        "tags": [
          "Interact"
        ],
        "requestBody": {
          "required": true,
          "description": "Vote confirmation data including user ID, video ID, vote value, and puzzle solution",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VoteModel"
              },
              "example": {
                "userId": "user123",
                "videoId": "kxOuG8jMIgI",
                "value": 1,
                "solution": "base64encodedpuzzlesolution"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/VoteModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/VoteModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vote successfully confirmed"
          },
          "400": {
            "description": "Bad Request - Invalid confirmation data, missing puzzle solution, or vote not found"
          },
          "401": {
            "description": "Unauthorized - Authentication required or invalid credentials"
          }
        }
      }
    },
