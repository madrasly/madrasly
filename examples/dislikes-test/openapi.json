    "/Interact/vote": {
      "post": {
        "summary": "Submit a vote for a YouTube video",
        "description": "Submits a like or dislike vote for a specified YouTube video. This endpoint is used to record user votes in the Return YouTube Dislike system. Requires a VoteModel containing the user ID, video ID, vote value (typically 1 for like, -1 for dislike), and optionally a puzzle solution for verification.",
        "tags": [
          "Interact"
        ],
        "requestBody": {
          "required": true,
          "description": "Vote data including user ID, video ID, vote value, and optional puzzle solution",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "type": "string",
                    "nullable": true
                  },
                  "videoId": {
                    "type": "string",
                    "nullable": true
                  },
                  "value": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "solution": {
                    "type": "string",
                    "format": "byte",
                    "nullable": true
                  }
                },
                "additionalProperties": false
              },
              "example": {
                "userId": "user123",
                "videoId": "kxOuG8jMIgI",
                "value": 1,
                "solution": null
              }
            },
            "text/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "type": "string",
                    "nullable": true
                  },
                  "videoId": {
                    "type": "string",
                    "nullable": true
                  },
                  "value": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "solution": {
                    "type": "string",
                    "format": "byte",
                    "nullable": true
                  }
                },
                "additionalProperties": false
              }
            },
            "application/*+json": {
              "schema": {
                "type": "object",
                "properties": {
                  "userId": {
                    "type": "string",
                    "nullable": true
                  },
                  "videoId": {
                    "type": "string",
                    "nullable": true
                  },
                  "value": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "solution": {
                    "type": "string",
                    "format": "byte",
                    "nullable": true
                  }
                },
                "additionalProperties": false
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Vote successfully submitted"
          },
          "400": {
            "description": "Bad Request - Invalid vote data or missing required fields"
          },
          "401": {
            "description": "Unauthorized - Authentication required or invalid credentials"
          }
        }
      }
    },
    "/Interact/confirmVote": {
